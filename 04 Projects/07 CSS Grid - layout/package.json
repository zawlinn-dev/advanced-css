{
  "name": "07-css-grid---layout",
  "version": "1.0.0",
  "description": "Layout by css grid",
  "main": "index.js",
  "scripts": {
    "dev": "live-server",
    "watch:scss": "node-sass assets/scss/main.scss assets/css/main.css -w",
    "start": "npm-run-all --parallel dev watch:scss",
    "compile:scss": "node-sass ./assets/scss/main.scss ./assets/css/main.css",
    "prefix:css": "postcss --use autoprefixer -b '> 1%, last 10 version' ./assets/css/main.css -o ./assets/css/main.prefix.css"
  },
  "author": "jonnas schmedtmann",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concat": "^1.0.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0"
  }
}
